  <phrase title="bdapi_consumer_app_name" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[App Name]]></phrase>
  <phrase title="bdapi_consumer_autoLoginJs" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[Auto login via JS]]></phrase>
  <phrase title="bdapi_consumer_autoLoginJs_explain" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[List of providers will be included in automatic remote login status detection by JavaScript. If user has already logged in with one of these, the page will be refreshed to reflect the synchronized session. This option cannot be changed here, you must <a href="admin.php?connected-accounts/" target="_blank" tabindex="-1">edit each provider</a> to turn it on or off.]]></phrase>
  <phrase title="bdapi_consumer_auto_login_user_x_requires_tfa" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[Welcome back {username}, you need to finish <a href="{twoStepLink}">two step verification</a> to login.]]></phrase>
  <phrase title="bdapi_consumer_auto_login_with_x_succeeded_y" version_id="1000010" version_string="1.0.0 Alpha"><![CDATA[Welcome back {username}, you have been logged in via {provider}.]]></phrase>
